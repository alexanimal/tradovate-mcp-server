# Tradovate API Credentials
# Rename this file to .env and fill in your credentials

# API Environment (demo, live)
TRADOVATE_API_ENVIRONMENT=demo

# Your Tradovate account username
TRADOVATE_USERNAME=your_username

# Your Tradovate account password
TRADOVATE_PASSWORD=your_password

# Your Tradovate API application ID
TRADOVATE_APP_ID=your_app_id

# Your Tradovate API application version
TRADOVATE_APP_VERSION=1.0.0

# Your Tradovate device ID (from API settings screen in your dashboard)
TRADOVATE_DEVICE_ID=your_device_id

# Your Tradovate API client ID
TRADOVATE_CID=your_cid

# Your Tradovate API secret key
TRADOVATE_SECRET=your_secret